<!doctype html>
<html class="no-js" lang="">
    <head>
        <meta name="robots" content="noindex,nofollow" />
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <title>Standard RegionalSite</title>
        <meta name="description" content="Standard RegionalSite template for use with balihoo-creative">
        <meta name="viewport" content="width=device-width, initial-scale=1">

        <link rel="icon" href="{{assets.img.favicon}}" type="image/x-icon" />

        <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.2/css/bootstrap.min.css">
        <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
        <link rel="stylesheet" href="{{assets.css.main}}">


        <script>
          //////////////////////////////
          //////////////////////////////
          //////////////////////////////
          /// NOT FOR PRODUCTION RELEASE
          partners = [];
          {{#affiliates}}
          partners.push({
            "DisplayName": "{{{DisplayName}}}",
            "Address": "{{{Address}}}",
            "Phone": "{{{Phone}}}",
            "AffiliateID":"{{{AffiliateID}}}",
            "SourceAffiliateNumber":"{{{SourceAffiliateNumber}}}",
            "RegionalSiteURL":"{{{RegionalSiteURL}}}",
            "Logo":"{{{Logo}}}",
            "Image":"{{{Image}}}",
            "Email":"{{{Email}}}",
            "Web":"{{{Web}}}",
            "Zip01":"{{{Zip01}}}",
            "State01":"{{{State01}}}",
            "City01":"{{{City01}}}",
            "Address02":"{{{Address02}}}",
            "LastName":"{{{LastName}}}",
            "FirstName":"{{{FirstName}}}"
          });
          {{/affiliates}}
        </script>

        <script src="{{assets.js.vendor.modernizr}}"></script>
    </head>
    <body>
        <!--[if lt IE 8]>
            <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
        <![endif]-->

        <!-- Page layout -->
        {{#request}}
          {{#ifpage.index}}{{> index}}{{/ifpage.index}}
          {{#ifpage.notfound}}{{> notfound}}{{/ifpage.notfound}}
        {{/request}}

        <script src="{{assets.js.vendor.jquery}}"></script>
        <script src="//maps.googleapis.com/maps/api/js"></script>
        <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.2/js/bootstrap.min.js"></script>
        <script src="{{assets.js.vendor.validator}}"></script>
        <script src="{{assets.js.plugins}}"></script>
        <script src="{{assets.js.main}}"></script>

        <!-- Google Analytics: change UA-XXXXX-X to be your site's ID. -->
        <script>
            (function(b,o,i,l,e,r){b.GoogleAnalyticsObject=l;b[l]||(b[l]=
            function(){(b[l].q=b[l].q||[]).push(arguments)});b[l].l=+new Date;
            e=o.createElement(i);r=o.getElementsByTagName(i)[0];
            e.src='https://www.google-analytics.com/analytics.js';
            r.parentNode.insertBefore(e,r)}(window,document,'script','ga'));

            ga('create','{{{GoogleAccount}}}','auto');
            ga('send','pageview');
        </script>
        {{{$tests}}}
    </body>
</html>
